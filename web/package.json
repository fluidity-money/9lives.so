    "keccak": "^3.0.4",
    "next": "15.2.4",
    "posthog-js": "^1.242.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.3",
    "react-hot-toast": "^2.5.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "thirdweb": "^5.99.1",
    "zod": "^3.24.4",
    "zustand": "5.0.0-rc.2"
