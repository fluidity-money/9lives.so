    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "cross-env": "^7.0.3",
    "eight-colors": "^1.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-playwright": "^2.2.0",
